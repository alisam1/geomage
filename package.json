{
  "name": "geomage",
  "version": "1.0.0",
  "description": "Geomage",
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/sass/**/*.scss\" --syntax scss",
    "test": "npm run editorconfig && npm run stylelint",
    "build": "gulp build",
    "start": "npm run build && gulp serve",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "keywords": [
    "Geomage"
  ],
  "author": "Mariya M",
  "license": "ISC",
  "browserslist": [
    "last 2 versions",
    "IE 11",
    "Firefox ESR"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/sass/**/*.{sass,scss}"
  ],
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "gh-pages": "^6.3.0",
    "gulp": "^5.0.1",
    "gulp-imagemin": "^9.1.0",
    "gulp-sass": "^6.0.1",
    "posthtml-include": "^2.0.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "browser-sync": "^3.0.4",
    "del": "^8.0.0",
    "font-awesome": "^4.7.0",
    "gulp-csso": "^4.0.1",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^10.0.0",
    "gulp-posthtml": "^3.0.5",
    "gulp-rename": "^2.1.0",
    "gulp-svgstore": "^9.0.0",
    "gulp-webp": "^5.0.0",
    "postcss": "^8.5.6"
  }
}
